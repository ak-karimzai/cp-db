ers\",\n  data() {\n    return {\n      users: [],\n      ready: false,\n      store,\n    };\n  },\n  beforeMount() {\n    Security.isAdmin();\n\n    fetch(\n      `${process.env.VUE_APP_API}/users`,\n      Security.requestOptionsWithoutBody()\n    )\n      .then((response) => response.json())\n      .then((response) => {\n        if (response.error) {\n          this.$emit('error', response.error.message);\n        } else {\n          this.users = response.users;\n        }\n      }).catch(error => {\n        this.$emit('error', error.message);\n      });\n  },\n};\n</script>"],"mappings":"AAiCA,OAAOA,QAAO,MAAO,eAAe;AACpC,SAASC,KAAI,QAAS,YAAY;AAClC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,KAAK;MACZJ;IACF,CAAC;EACH,CAAC;EACDK,WAAWA,CAAA,EAAG;IACZN,QAAQ,CAACO,OAAO,CAAC,CAAC;IAElBC,KAAK,CACF,GAAEC,OAAO,CAACC,GAAG,CAACC,WAAY,QAAO,EAClCX,QAAQ,CAACY,yBAAyB,CAAC,CACrC,EACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAIA,QAAQ,CAACE,KAAK,EAAE;QAClB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAEH,QAAQ,CAACE,KAAK,CAACE,OAAO,CAAC;MAC7C,OAAO;QACL,IAAI,CAACd,KAAI,GAAIU,QAAQ,CAACV,KAAK;MAC7B;IACF,CAAC,CAAC,CAACe,KAAK,CAACH,KAAI,IAAK;MAChB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAED,KAAK,CAACE,OAAO,CAAC;IACpC,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}