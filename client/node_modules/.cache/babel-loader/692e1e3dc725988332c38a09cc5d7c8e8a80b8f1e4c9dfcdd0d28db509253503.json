{"ast":null,"code":"import Security from \"./security\";\nexport default {\n  name: \"BillEdit\",\n  data() {\n    return {\n      ready: false,\n      bill: {\n        id: 0,\n        from: \"\",\n        until: \"\",\n        spend_amount: 0.0,\n        payment_amount: 0.0,\n        paid: \"false\"\n      },\n      apartments: [],\n      services: []\n    };\n  },\n  methods: {\n    getDate(dateString) {\n      const date = new Date(dateString);\n      const year = date.getFullYear();\n      const month = String(date.getMonth() + 1).padStart(2, \"0\");\n      const day = String(date.getDate()).padStart(2, \"0\");\n      return `${year}-${month}-${day}`;\n    }\n  },\n  beforeMount() {\n    if (this.$route.params.billId != 0) {\n      fetch(`${process.env.VUE_APP_SERVER_API}/bills/${this.$route.params.billId}`, Security.requestOptionsWithoutBody()).then(res => res.json()).then(res => {\n        if (res.error) {\n          this.$emit(\"error\", res.error.message);\n        } else {\n          this.bill = res.data;\n          this.bill.from = getDate(this.bill.from);\n          this.bill.until = getDate(this.bill.until);\n          console.log(this.bill);\n          this.ready = true;\n        }\n      }).catch(err => {\n        this.$emit(\"error\", err);\n      });\n    } else {\n      this.ready = true;\n    }\n  },\n  methods: {\n    submitHandler() {\n      if (this.from == \"\" || this.until == \"\") {\n        this.$emit(\"error\", \"All fields are required\");\n        return;\n      }\n      const payload = {\n        id: this.bill.id,\n        from: new Date(this.bill.from).toISOString(),\n        until: new Date(this.bill.until).toISOString(),\n        spend_amount: this.bill.spend_amount,\n        apartment_id: this.bill.apartment_id,\n        service_id: this.bill.service_id\n      };\n      console.log(payload);\n      // fetch(\n      //   `${process.env.VUE_APP_SERVER_API}/bills`,\n      //   Security.requestOptions(payload)\n      // )\n      //   .then((res) => res.json())\n      //   .then((res) => {\n      //     if (res.error) {\n      //       this.$emit(\"error\", res.error.message);\n      //     } else {\n      //       this.$emit(\"success\", \"Changed successfully\");\n      //       this.$emit(\"forceUpdate\");\n      //     }\n      //   })\n      //   .catch((err) => this.$emit(\"error\", err));\n    }\n  }\n};","map":{"version":3,"names":["Security","name","data","ready","bill","id","from","until","spend_amount","payment_amount","paid","apartments","services","methods","getDate","dateString","date","Date","year","getFullYear","month","String","getMonth","padStart","day","beforeMount","$route","params","billId","fetch","process","env","VUE_APP_SERVER_API","requestOptionsWithoutBody","then","res","json","error","$emit","message","console","log","catch","err","submitHandler","payload","toISOString","apartment_id","service_id"],"sources":["/home/khalid/uni/6thsem/cp_db/new_code/client/src/components/BillEdit.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"col\">\n      <div class=\"row\">\n        <h1 class=\"mt-3\">\n          {{ this.bill.id === 0 ? \"Create\" : \"Update\" }} Bill\n        </h1>\n        <hr />\n        <form @submit.prevent=\"submitHandler\" v-if=\"this.ready\" event=\"myevent\">\n          <label for=\"from\" class=\"form-label\">From:</label>\n          <input\n            v-model=\"bill.from\"\n            type=\"date\"\n            id=\"from\"\n            class=\"form-control\"\n            required\n          />\n\n          <label for=\"until\" class=\"form-label\">Until:</label>\n          <input\n            v-model=\"bill.until\"\n            type=\"date\"\n            id=\"until\"\n            class=\"form-control\"\n            required\n          />\n\n          <label for=\"spend_amount\" class=\"form-label\">Spend Amount</label>\n          <input\n            type=\"number\"\n            id=\"spend_amount\"\n            class=\"form-control\"\n            v-model=\"bill.spend_amount\"\n            step=\"0.01\"\n          />\n\n          <div v-if=\"this.$route.params.billId != 0\">\n            <label for=\"payment_amount\" class=\"form-label\"\n              >Payment Amount</label\n            >\n            <label id=\"payment_amount\" class=\"form-control\">{{\n              bill.payment_amount\n            }}</label>\n          </div>\n          <hr />\n          <div class=\"float-start\">\n            <input\n              type=\"submit\"\n              class=\"btn btn-primary me-2\"\n              @click=\"submitHandler\"\n              :value=\"this.bill.id === 0 ? 'Create' : 'Update'\"\n            />\n            <router-link to=\"/admin/bills\" class=\"btn btn-outline-secondary\"\n              >Cancel</router-link\n            >\n          </div>\n        </form>\n        <p v-else>Loading...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Security from \"./security\";\nexport default {\n  name: \"BillEdit\",\n  data() {\n    return {\n      ready: false,\n      bill: {\n        id: 0,\n        from: \"\",\n        until: \"\",\n        spend_amount: 0.0,\n        payment_amount: 0.0,\n        paid: \"false\",\n      },\n      apartments: [],\n      services: [],\n    };\n  },\n  methods: {\n    getDate(dateString) {\n      const date = new Date(dateString);\n\n      const year = date.getFullYear();\n      const month = String(date.getMonth() + 1).padStart(2, \"0\");\n      const day = String(date.getDate()).padStart(2, \"0\");\n      return `${year}-${month}-${day}`;\n    },\n  },\n  beforeMount() {\n    if (this.$route.params.billId != 0) {\n      fetch(\n        `${process.env.VUE_APP_SERVER_API}/bills/${this.$route.params.billId}`,\n        Security.requestOptionsWithoutBody()\n      )\n        .then((res) => res.json())\n        .then((res) => {\n          if (res.error) {\n            this.$emit(\"error\", res.error.message);\n          } else {\n            this.bill = res.data;\n            this.bill.from = getDate(this.bill.from);\n            this.bill.until = getDate(this.bill.until);\n            console.log(this.bill);\n            this.ready = true;\n          }\n        })\n        .catch((err) => {\n          this.$emit(\"error\", err);\n        });\n    } else {\n      this.ready = true;\n    }\n  },\n  methods: {\n    submitHandler() {\n      if (this.from == \"\" || this.until == \"\") {\n        this.$emit(\"error\", \"All fields are required\");\n        return;\n      }\n      const payload = {\n        id: this.bill.id,\n        from: new Date(this.bill.from).toISOString(),\n        until: new Date(this.bill.until).toISOString(),\n        spend_amount: this.bill.spend_amount,\n        apartment_id: this.bill.apartment_id,\n        service_id: this.bill.service_id,\n      };\n      console.log(payload);\n      // fetch(\n      //   `${process.env.VUE_APP_SERVER_API}/bills`,\n      //   Security.requestOptions(payload)\n      // )\n      //   .then((res) => res.json())\n      //   .then((res) => {\n      //     if (res.error) {\n      //       this.$emit(\"error\", res.error.message);\n      //     } else {\n      //       this.$emit(\"success\", \"Changed successfully\");\n      //       this.$emit(\"forceUpdate\");\n      //     }\n      //   })\n      //   .catch((err) => this.$emit(\"error\", err));\n    },\n  },\n};\n</script>"],"mappings":"AAgEA,OAAOA,QAAO,MAAO,YAAY;AACjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,KAAK;MACZC,IAAI,EAAE;QACJC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,YAAY,EAAE,GAAG;QACjBC,cAAc,EAAE,GAAG;QACnBC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAACC,UAAU,EAAE;MAClB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MAEjC,MAAMG,IAAG,GAAIF,IAAI,CAACG,WAAW,CAAC,CAAC;MAC/B,MAAMC,KAAI,GAAIC,MAAM,CAACL,IAAI,CAACM,QAAQ,CAAC,IAAI,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG