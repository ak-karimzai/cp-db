ult {\n  name: \"ClientBills\",\n  data() {\n    return {\n      payments: [],\n      ready: false,\n      store,\n    };\n  },\n  beforeMount() {\n    fetch(\n      `${process.env.VUE_APP_SERVER_API}/payments?userId=${this.store.user.id}`,\n      Security.requestOptionsWithoutBody()\n    )\n      .then((response) => response.json())\n      .then((response) => {\n        if (response.error) {\n          this.$emit(\"error\", response.error.message);\n        } else {\n          this.payments = response.data;\n          this.ready = true;\n        }\n      })\n      .catch((error) => {\n        this.$emit(\"error\", error.message);\n      });\n  },\n};\n</script>"],"mappings":"AA8BA,OAAOA,QAAO,MAAO,eAAe;AACpC,SAASC,KAAI,QAAS,YAAY;AAClC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,KAAK;MACZJ;IACF,CAAC;EACH,CAAC;EACDK,WAAWA,CAAA,EAAG;IACZC,KAAK,CACF,GAAEC,OAAO,CAACC,GAAG,CAACC,kBAAmB,oBAAmB,IAAI,CAACT,KAAK,CAACU,IAAI,CAACC,EAAG,EAAC,EACzEZ,QAAQ,CAACa,yBAAyB,CAAC,CACrC,EACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAIA,QAAQ,CAACE,KAAK,EAAE;QAClB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAEH,QAAQ,CAACE,KAAK,CAACE,OAAO,CAAC;MAC7C,OAAO;QACL,IAAI,CAACf,QAAO,GAAIW,QAAQ,CAACZ,IAAI;QAC7B,IAAI,CAACE,KAAI,GAAI,IAAI;MACnB;IACF,CAAC,EACAe,KAAK,CAAEH,KAAK,IAAK;MAChB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAED,KAAK,CAACE,OAAO,CAAC;IACpC,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}