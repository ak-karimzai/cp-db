nexport default {\n  name: \"ClientBill\",\n  data() {\n    return {\n      bill: {},\n      ready: false,\n      store,\n    };\n  },\n  beforeMount() {\n    fetch(\n      `${process.env.VUE_APP_SERVER_API}/bills/${this.$route.params.billId}`,\n      Security.requestOptionsWithoutBody()\n    )\n      .then((response) => response.json())\n      .then((response) => {\n        if (response.error) {\n          this.$emit(\"error\", response.error.message);\n        } else {\n          this.bill = response.data;\n          this.ready = true;\n        }\n      })\n      .catch((error) => {\n        this.$emit(\"error\", error.message);\n      });\n  },\n};\n</script>"],"mappings":"AA6BA,OAAOA,QAAO,MAAO,eAAe;AACpC,SAASC,KAAI,QAAS,YAAY;AAClC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,KAAK,EAAE,KAAK;MACZJ;IACF,CAAC;EACH,CAAC;EACDK,WAAWA,CAAA,EAAG;IACZC,KAAK,CACF,GAAEC,OAAO,CAACC,GAAG,CAACC,kBAAmB,UAAS,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,MAAO,EAAC,EACtEb,QAAQ,CAACc,yBAAyB,CAAC,CACrC,EACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAIA,QAAQ,CAACE,KAAK,EAAE;QAClB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAEH,QAAQ,CAACE,KAAK,CAACE,OAAO,CAAC;MAC7C,OAAO;QACL,IAAI,CAAChB,IAAG,GAAIY,QAAQ,CAACb,IAAI;QACzB,IAAI,CAACE,KAAI,GAAI,IAAI;MACnB;IACF,CAAC,EACAgB,KAAK,CAAEH,KAAK,IAAK;MAChB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAED,KAAK,CAACE,OAAO,CAAC;IACpC,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}