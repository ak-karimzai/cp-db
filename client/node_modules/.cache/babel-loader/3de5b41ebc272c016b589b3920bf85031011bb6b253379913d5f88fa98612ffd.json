{"ast":null,"code":"import Security from \"./security\";\nimport { store } from \"./store.js\";\nexport default {\n  name: \"ClientApartments\",\n  data() {\n    return {\n      ready: false,\n      apartments: [],\n      store\n    };\n  },\n  beforeMount() {\n    fetch(`${process.env.VUE_APP_SERVER_API}/apartments?userId=${this.store.user.id}`, Security.requestOptionsWithoutBody()).then(response => response.json()).then(response => {\n      if (response.error) {\n        this.$emit(\"error\", response.error.message);\n      } else {\n        this.apartments = response.data;\n        this.ready = true;\n      }\n    }).catch(err => this.$emit(\"error\", err));\n  }\n};","map":{"version":3,"names":["Security","store","name","data","ready","apartments","beforeMount","fetch","process","env","VUE_APP_SERVER_API","user","id","requestOptionsWithoutBody","then","response","json","error","$emit","message","catch","err"],"sources":["/home/khalid/uni/6thsem/cp_db/new_code/client/src/components/ClientApartments.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col\"></div>\n      <table v-if=\"this.ready\" class=\"table table-compact table-scriped\">\n        <thead>\n          <th>Size</th>\n          <th>Room Number</th>\n          <th>Tenant</th>\n        </thead>\n        <tbody>\n          <tr v-for=\"apartment in apartments\" :key=\"apartment.id\">\n            <td>\n              <router-link\n                class=\"text-decoration-none\"\n                :to=\"`/bills/:aprId=${apartment.id}`\"\n              >\n                {{ apartment.size }}\n              </router-link>\n            </td>\n            <td>{{ apartment.room_numbers }}</td>\n            <td>\n              {{ apartment.user.last_name }} ,\n              {{ apartment.user.first_name }}\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Security from \"./security\";\nimport { store } from \"./store.js\";\n\nexport default {\n  name: \"ClientApartments\",\n  data() {\n    return {\n      ready: false,\n      apartments: [],\n      store,\n    };\n  },\n  beforeMount() {\n    fetch(\n      `${process.env.VUE_APP_SERVER_API}/apartments?userId=${this.store.user.id}`,\n      Security.requestOptionsWithoutBody()\n    )\n      .then((response) => response.json())\n      .then((response) => {\n        if (response.error) {\n          this.$emit(\"error\", response.error.message);\n        } else {\n          this.apartments = response.data;\n          this.ready = true;\n        }\n      })\n      .catch((err) => this.$emit(\"error\", err));\n  },\n};\n</script>"],"mappings":"AAiCA,OAAOA,QAAO,MAAO,YAAY;AACjC,SAASC,KAAI,QAAS,YAAY;AAElC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,KAAK;MACZC,UAAU,EAAE,EAAE;MACdJ;IACF,CAAC;EACH,CAAC;EACDK,WAAWA,CAAA,EAAG;IACZC,KAAK,CACF,GAAEC,OAAO,CAACC,GAAG,CAACC,kBAAmB,sBAAqB,IAAI,CAACT,KAAK,CAACU,IAAI,CAACC,EAAG,EAAC,EAC3EZ,QAAQ,CAACa,yBAAyB,CAAC,CACrC,EACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAClCF,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAIA,QAAQ,CAACE,KAAK,EAAE;QAClB,IAAI,CAACC,KAAK,CAAC,OAAO,EAAEH,QAAQ,CAACE,KAAK,CAACE,OAAO,CAAC;MAC7C,OAAO;QACL,IAAI,CAACd,UAAS,GAAIU,QAAQ,CAACZ,IAAI;QAC/B,IAAI,CAACC,KAAI,GAAI,IAAI;MACnB;IACF,CAAC,EACAgB,KAAK,CAAEC,GAAG,IAAK,IAAI,CAACH,KAAK,CAAC,OAAO,EAAEG,GAAG,CAAC,CAAC;EAC7C;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}